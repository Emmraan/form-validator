{
  "version": 2,
  "installCommand": "pnpm install",
  "buildCommand": "pnpm run build",
  "functions": {
    "api/index.js": {
      "runtime": "nodejs18.x",
      "includeFiles": "dist/**",
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "env": {
    "RUNTIME": "vercel"
  },
  "cleanUrls": false,
  "tracing": {
    "enabled": true
  }
}